DATA_PATH=data/cnn_dm-large
MODEL=bart.large.cnn_dm/model.pt
NUM_PARALLEL=3
TEST_FILE=test.source
HYPO_NAME=test.hypo_large
GPU=7

python inference/bart_large_inference_parallel.py --model $MODEL \
 --data-path $DATA_PATH\
 --parallel $NUM_PARALLEL\
 --test-file $TEST_FILE\
 --hypo $HYPO_NAME\
 --gpu $GPU;

python inference/run_rouge_score.py $HYPO_NAME $DATA_PATH
